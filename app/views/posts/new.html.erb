<%= form_with(model: [current_user, Post.new]) do |form| %>
    <div>
      <%= form.label :title, 'Title' %>
      <%= form.text_field :title %>
    </div>
  
    <div>
      <%= form.label :text, 'Content' %>
      <%= form.text_area :text %>
    </div>
  
    <div>
      <%= form.submit 'Create Post' %>
    </div>
  <% end %>